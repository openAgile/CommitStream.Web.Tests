    "mocha": "^2.4.5",
    "mocha-tap-reporter": "^0.1.3",
    "ramda": "^0.13.0",
    "request": "^2.69.0",
    "request-promise": "^2.0.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "underscore": "^1.8.3",
    "uuid-v4": "^0.1.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "axios": "^0.15.2",
    "babel-cli": "^6.4.5",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-transform-runtime": "^6.15.0",
